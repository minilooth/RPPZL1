<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sample.models.Marialstatus" table="marialstatus" schema="bank">
        <id name="statusId">
            <column name="StatusId" sql-type="int"/>
        </id>
        <property name="statusName">
            <column name="StatusName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>